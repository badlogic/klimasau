{
    "manifest_version": 3,
    "name": "Klimasau",
    "version": "1.0",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["content.js"]
      }
    ],
    "web_accessible_resources": [
        {
          "resources": ["piggy_sheet.png", "scream.mp3", "tini.mp3", "tini-2.mp3"],
          "matches": ["<all_urls>"]
        }
      ]
  }